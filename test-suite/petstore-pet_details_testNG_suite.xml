<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Restful Employee Test Suite" thread-count="3" parallel="tests">
    <listeners>
        <listener
                class-name="au.com.log.TestListener"/>
    </listeners>
    <test name="Restful Employee tests">
        <classes>
            <class name="au.com.restful.RestfulCrudPetDetailsTests">
                <methods>
                    <include name="createPetDetailsCategoryTest"/>
                    <include name="updatePetDetailsCategoryTest"/>
                    <include name="createPetDetailsTest"/>
                    <include name="createPetImageTest"/>
                    <include name="getPetDetailsTest"/>
                    <include name="getPetDetailsByStatusTest"/>
                    <include name="deleteUserTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>